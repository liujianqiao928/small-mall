<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pc.small.mapper.ShopTypeDao">
    <select id="findBuTypeName" resultType="string" parameterType="string">
        select FLID from SHOPTYPE where FLNAME=#{name}
    </select>
<select id="findLikeName" resultType="string" parameterType="string">
    select FLID from SHOPTYPE where FLNAME LIKE '%${serchtext}%'
</select>
    <select id="findById" resultType="string" parameterType="string">
        select FLNAME from SHOPTYPE where FLID=#{types}
    </select>
</mapper>