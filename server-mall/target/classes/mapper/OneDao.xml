<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="server.mall.mapper.OneDao">
    <select id="findByName" resultType="string" parameterType="string">
        select OID from ONE where ONAME=#{name}
    </select>
    <select id="findLikeName" resultType="string" parameterType="string">
    select OID from ONE where ONAME LIKE '%${serchtext}%'
</select>
    <select id="findById" resultType="string" parameterType="string">
        select ONAME from ONE where OID =#{one}
    </select>
</mapper>