<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="server.mall.mapper.TwoDao">
    <select id="findByName" resultType="string" parameterType="string">
        select TID from TWO where TWNAME=#{name}
    </select>
    <select id="findLikeName" resultType="string" parameterType="string">
    select TID from TWO where TWNAME LIKE'%${serchtext}%'
</select>
    <select id="findById" parameterType="string" resultType="string">
        select TWNAME from TWO where TID=#{two}
    </select>
</mapper>